# Mostly stolen from Crypt::Twofish2.

use strict;

BEGIN { $| = 1; print "1..78\n"; }

use Crypt::Twofish_PP;

my $i = 1;

# now try the test vectors provided
while (<DATA>) {
   if (/KEY=(\S+)\s+PT=(\S+)\s+CT=(\S+)\s+/) {
      my $key = pack "H*", $1;
      my $pt  = pack "H*", $2;
      my $ct  = pack "H*", $3;

      my $ecb = new Crypt::Twofish_PP $key;

      my $crypted = $ecb->encrypt($pt);
      print $crypted eq $ct ? "" : "not ", "ok ".$i++."\n";
      print $ecb->decrypt($crypted) eq $pt ? "" : "not ", "ok ".$i++."\n";
   }
}

